<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 宽高比布局自定义属性 -->
    <declare-styleable name="RationLayout">
        <!-- 宽高比 -->
        <attr name="ratio" format="float"/>
        <!--相对于  height/width-->
        <attr name="relative">
            <enum name="width" value="1"/>
            <enum name="height" value="2"/>
        </attr>
    </declare-styleable>
</resources>